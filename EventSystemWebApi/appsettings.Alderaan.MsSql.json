{
  "database": {
    // IP or Hostname of the Machine where the Database is Hosted
    // if omitted (commented out) it should default to localhost
    "Hostname": "(localdb)\\eventSystem",

    // Port for the Database Server
    //"Port": "3306",

    // Provider possible Values are "MySql" and "MsSql"
    // if omitted should default to "MySql"
    "Provider": "MsSql",

    // Name of the Database
    // if omitted should default to "db_EventSystem
    "Databasename": "db_EventSystem_TEST",

    // Only possible for MsSql Provider, if used no SqlUSername or SqlPassword will be required as the Client Authenticates with your Windows Account
    "UseWindowsAuthentication": true

    //"SqlUsername": "",

    //"SqlPassword": ""
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}